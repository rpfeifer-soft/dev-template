{
   "name": "dev-template",
   "version": "1.0.0",
   "description": "Template to start a new web-application (Docker+Rollup+WebSocket+Typescript)!",
   "type": "module",
   "appid": "dev",
   "title": "Development template",
   "dockerAccount": "rpfeifer/docker-image",
   "githubAccount": "https://github.com/rpfeifer-soft",
   "scripts": {
      "clean": "docker-compose down && rimraf ./dist",
      "i18n": "node i18n.js check",
      "translate": "node i18n update && node i18n.js translate",
      "compile": "npm run i18n && tsc -b src",
      "lint": "eslint src -c .eslintrc.cjs",
      "bundle": "tsc -b src && rollup -c -m --environment DEVENV",
      "watch": "tsc -b -w src & rollup -c -m -w",
      "devenv": "npm run compile && rollup -c -m --environment DEVENV && docker-compose build && (npm run watch & docker-compose up)",
      "nodemon": "nodemon",
      "start": "node dist/server/bundle.js",
      "list": "ps aux | grep ${npm_package_name}",
      "test:cycle": "npm run compile && madge --image graph.svg ./dist/client/index.js",
      "test:compile": "tsc --project ./src/tsconfig.test.json",
      "test:bundle": "rollup -c ./src/rollup.test.js",
      "test:run": "node ./test/all.js",
      "test": "npm run test:compile && npm run test:bundle && npm run test:run && rimraf ./test",
      "stage": "(npm run lint && node i18n.js check && node i18n.js update && git add -A) || npm run translate",
      "check": "npm run compile && npm run test",
      "build": "npm run compile && rollup -c",
      "publish": "npm run lint && npm run test && docker build . --file src/Dockerfile --no-cache --tag ${npm_package_dockerAccount}:${npm_package_appid} && docker push ${npm_package_dockerAccount}:${npm_package_appid}"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/rpfeifer-soft/dev-template.git"
   },
   "author": "Ren√© Pfeifer",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/rpfeifer-soft/dev-template/issues"
   },
   "homepage": "https://github.com/rpfeifer-soft/dev-template#readme",
   "dependencies": {
      "device-detector-js": "^2.2.5",
      "express": "^4.17.1",
      "path-to-regexp": "^6.2.0",
      "ts-node": "^8.10.2",
      "ttag": "^1.7.23",
      "ws": "^7.4.0"
   },
   "devDependencies": {
      "@rollup/plugin-commonjs": "^12.0.0",
      "@rollup/plugin-json": "^4.1.0",
      "@rollup/plugin-multi-entry": "^3.0.1",
      "@rollup/plugin-node-resolve": "^8.4.0",
      "@rollup/plugin-replace": "^2.3.4",
      "@types/express": "^4.17.9",
      "@types/node": "^14.14.10",
      "@types/tape": "^4.13.0",
      "@types/ws": "^7.4.0",
      "@typescript-eslint/eslint-plugin": "^3.10.1",
      "@typescript-eslint/parser": "^3.10.1",
      "eslint": "^7.14.0",
      "eslint-config-google": "^0.14.0",
      "eslint-config-prettier": "^6.15.0",
      "eslint-plugin-jsdoc": "^26.0.2",
      "eslint-plugin-unicorn": "^23.0.0",
      "glob": "^7.1.6",
      "madge": "^4.0.1",
      "prettier": "^2.2.1",
      "rimraf": "^3.0.2",
      "rollup": "^2.34.0",
      "rollup-plugin-external-globals": "^0.6.1",
      "rollup-plugin-sourcemaps": "^0.6.3",
      "rollup-plugin-terser": "^5.3.1",
      "tape": "^5.0.1",
      "ttag-cli": "^1.8.8",
      "typescript": "^3.9.7",
      "wscat": "^4.0.1",
      "yargs": "^16.1.1"
   }
}
