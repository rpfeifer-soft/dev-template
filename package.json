{
   "name": "dev-template",
   "version": "1.0.0",
   "description": "Template to start a new web-application (Docker+Rollup+WebSocket+Typescript)!",
   "type": "module",
   "appid": "dev",
   "title": "Development template",
   "dockerAccount": "rpfeifer/team-manager",
   "prodServer": "iserver.local",
   "prodBase": "Desktop/dev-template",
   "githubAccount": "https://github.com/rpfeifer-soft",
   "scripts": {
      "clean": "docker-compose down && rimraf ./dist",
      "compile": "tsc -b src",
      "lint": "eslint src/**/*.ts",
      "watch": "tsc -b -w src & rollup -c -w",
      "devenv": "npm run compile && rollup -c --environment DEVENV && docker-compose build && (npm run watch & docker-compose up)",
      "nodemon": "nodemon",
      "start": "node dist/server/start.js",
      "list": "ps aux | grep ${npm_package_name}",
      "test": "tsc --project ./src/tsconfig.test.json && rollup -c ./src/rollup.test.js && node ./test/all.js && rimraf ./test",
      "check": "npm run compile && npm run test",
      "build": "npm run compile && rollup -c",
      "publish": "npm run lint && npm run test && docker build . --file src/Dockerfile --no-cache --tag ${npm_package_dockerAccount}:${npm_package_appid} && docker push ${npm_package_dockerAccount}:${npm_package_appid}",
      "scp": "scp -r install/secrets.json rpfeifer@${npm_package_prodServer}:${npm_package_prodBase}/secrets.json",
      "ssh": "ssh ${npm_package_prodServer} -t \"cd ${npm_package_prodBase}; svn export --force ${npm_package_githubAccount}/${npm_package_name}/trunk/install .; bash --login; echo;\""
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/rpfeifer-soft/dev-template.git"
   },
   "author": "Ren√© Pfeifer",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/rpfeifer-soft/dev-template/issues"
   },
   "homepage": "https://github.com/rpfeifer-soft/dev-template#readme",
   "dependencies": {
      "express": "^4.17.1",
      "ws": "^7.3.0"
   },
   "devDependencies": {
      "@rollup/plugin-commonjs": "^12.0.0",
      "@rollup/plugin-multi-entry": "^3.0.1",
      "@rollup/plugin-node-resolve": "^8.0.0",
      "@types/express": "^4.17.6",
      "@types/node": "^14.0.1",
      "@types/tape": "^4.13.0",
      "@types/ws": "^7.2.4",
      "@typescript-eslint/eslint-plugin": "^3.0.2",
      "@typescript-eslint/parser": "^3.0.2",
      "eslint": "^7.2.0",
      "eslint-config-google": "^0.14.0",
      "eslint-config-prettier": "^6.11.0",
      "eslint-plugin-jsdoc": "^26.0.1",
      "prettier": "^2.0.5",
      "rimraf": "^3.0.2",
      "rollup": "^2.10.2",
      "rollup-plugin-external-globals": "^0.5.0",
      "rollup-plugin-terser": "^5.3.0",
      "tape": "^5.0.0",
      "typescript": "^3.9.2",
      "wscat": "^4.0.0"
   }
}
