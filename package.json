{
   "name": "dev-template",
   "version": "1.0.0",
   "description": "Template to start a new web-application (Docker+Rollup+Typescript)!",
   "main": "index.js",
   "type": "module",
   "scripts": {
      "clean": "docker-compose down && rimraf ./dist",
      "compile": "tsc -b src",
      "bundle": "rollup --config",
      "dockerize": "docker-compose build",
      "start:dev": "npm run compile && npm run bundle && npm run dockerize && (tsc -b src --watch & rollup --watch --config & docker-compose up)",
      "start:server": "nodemon",
      "test": "echo \"Error: no test specified\" && exit 1",
      "ssh": "ssh iserver.local -t \"cd Desktop/dev-template; svn export --force https://github.com/rpfeifer-soft/${npm_package_name}/trunk/package-server.json package.json; bash --login\""
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/rpfeifer-soft/dev-template.git"
   },
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/rpfeifer-soft/dev-template/issues"
   },
   "homepage": "https://github.com/rpfeifer-soft/dev-template#readme",
   "dependencies": {
      "express": "^4.17.1",
      "ws": "^7.3.0"
   },
   "devDependencies": {
      "@types/express": "^4.17.6",
      "@types/node": "^14.0.1",
      "@types/ws": "^7.2.4",
      "prettier": "^2.0.5",
      "rimraf": "^3.0.2",
      "rollup": "^2.10.2",
      "tslint": "^6.1.2",
      "tslint-react": "^5.0.0",
      "typescript": "^3.9.2",
      "wscat": "^4.0.0"
   }
}
